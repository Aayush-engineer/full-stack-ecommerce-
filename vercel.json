{
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "build/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.+\\.(js|css|png|jpg|jpeg|gif|ico|json|woff2|woff|ttf|svg))$", "dest": "/$1" },
    { "src": "/images/(.*)", "dest": "/images/$1" },
    { "src": "/(.*)", "dest": "/build/index.html" }
  ],
  "rewrites": [
    { "source": "/products", "destination": "/index.js" },
    { "source": "/categories", "destination": "/index.js" },
    { "source": "/brands", "destination": "/index.js" },
    { "source": "/users", "destination": "/index.js" },
    { "source": "/auth", "destination": "/index.js" },
    { "source": "/cart", "destination": "/index.js" },
    { "source": "/orders", "destination": "/index.js" },
    { "source": "/webhook", "destination": "/index.js" },
    { "source": "/create-payment-intent", "destination": "/index.js" }
  ]
}


